      "eslint",
      "git add"
    ],
    "./public/locales/": [
      "eslint --fix --ext .json"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4"
